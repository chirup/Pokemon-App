{
  "extends": ["react-app", "plugin:jsx-a11y/recommended", "eslint:recommended"],
  "plugins": ["jsx-a11y"],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  }
}
